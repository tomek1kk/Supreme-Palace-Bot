{
  "manifest_version": 2,

  "name": "SUPREME BOT",
  "description": "Tomasz Kostowski, Piotr Koskiewicz",
  "version": "1.1",

  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "128.png"
    },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Supreme bot"
  },
  "content_scripts": [
      {
          "matches": ["https://www.supremenewyork.com/*", "<all_urls>"],
          "js": ["test.js"]
      }
  ],
  "permissions": [
    "notifications",
    "activeTab",
    "storage",
    "tabs"
  ]
}
